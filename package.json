{
  "name": "windmill-dashboard-react",
  "version": "0.4.0",
  "description": "A multi theme, completely accessible, with components and pages examples, (almost) ready for production dashboard.",
  "private": true,
  "scripts": {
    "tailwind:dev": "tailwindcss -i src/assets/css/tailwind.css -o src/assets/css/tailwind.output.css --watch",
    "tailwind:build": "cross-env NODE_ENV=production postcss src/assets/css/tailwind.css -o src/assets/css/tailwind.output.css",
    "prestart": "yarn tailwind:dev",
    "prebuild": "yarn tailwind:build",
    "cz": "git-cz",
    "release": "release-it",
    "eject": "react-scripts eject",
    "start:dev": "craco --max_old_space_size=4096 start",
    "build": "craco --max_old_space_size=4096 build",
    "test": "craco test"
  },
  "dependencies": {
    "@craco/craco": "^6.3.0",
    "@dropzone-ui/react": "^5.5.6",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@tailwindcss/forms": "^0.3.3",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.5.0",
    "@testing-library/user-event": "7.2.1",
    "@types/node": "^16.6.0",
    "@types/react": "^16.14.13",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/styled-components": "^5.1.12",
    "@types/uuid": "^8.3.1",
    "@windmill/react-ui": "^0.6.0",
    "axios": "^0.24.0",
    "chart.js": "2.9.3",
    "classnames": "^2.3.1",
    "faker": "4.1.0",
    "firebase": "^8.9.1",
    "fishery": "^1.4.0",
    "framer-motion": "^4.1.17",
    "lodash": "^4.17.21",
    "mathjs": "^9.5.0",
    "planning-poker-client-sdk": "^1.2.3",
    "postcss-loader": "^6.1.1",
    "postcss-nested": "^5.0.6",
    "react": "17.0.0",
    "react-chartjs-2": "2.9.0",
    "react-dom": "17.0.0",
    "react-focus-lock": "2.4.0",
    "react-hook-form": "^7.17.0",
    "react-query": "^3.21.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "react-transition-group": "4.4.1",
    "smart-youtuber-client-sdk": "^1.0.12",
    "styled-components": "^5.3.0",
    "sweetalert2": "^11.1.7",
    "ts-pattern": "^3.3.3",
    "twin.macro": "^2.6.2",
    "typescript": "^4.3.5",
    "unique-names-generator": "^4.6.0",
    "url-join": "^4.0.1",
    "uuid": "^8.3.2",
    "webpack": "4.44.2",
    "zustand": "^3.6.4"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "1.1.4",
    "@svgr/webpack": "5.4.0",
    "@types/lodash": "^4.14.175",
    "@types/mathjs": "^9.4.2",
    "@types/url-join": "^4.0.1",
    "autoprefixer": "^9",
    "babel-plugin-styled-components": "^1.13.2",
    "commitizen": "4.1.2",
    "cross-env": "7.0.2",
    "cssnano": "^4.1.10",
    "cz-conventional-changelog": "3.2.0",
    "env-cmd": "^10.1.0",
    "git-scripts": "^0.4.3",
    "postcss": "^8.3.8",
    "postcss-cli": "^8.3.1",
    "prettier": "2.3.2",
    "react-axe": "3.5.2",
    "release-it": "13.6.4",
    "surge": "^0.23.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat"
  },
  "keywords": [
    "windmill",
    "dashboard",
    "admin",
    "tailwind",
    "react"
  ],
  "release-it": {
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "git": {
    "scripts": {
      "pre-push": "npm run build; surge --project ./build --domain planning-poker-app.surge.sh"
    }
  }
}
